    "type": "library",
    "keywords": [ "nette", "arachne", "symfony", "events" ],
    "license": "MIT",
    "authors": [
        {
            "name": "Jáchym Toušek",
            "homepage": "http://enumag.cz",
            "email": "enumag@gmail.com"
        }
    ],
    "require": {
        "php": "^5.4.0|^7.0.0",
        "arachne/container-adapter": "^0.1.0|^0.2.0",
        "nette/di": "^2.3.0",
        "nette/utils": "^2.3.0",
        "symfony/event-dispatcher": "^2.8.0|^3.0.0"
    },
    "require-dev": {
        "php": "^5.5.0|^7.0.0",
        "arachne/bootstrap": "^0.2.1",
        "codeception/codeception": "~2.2.0",
        "nette/application": "^2.3.0",
        "kdyby/console": "^2.6.0"
    },
    "autoload": {
        "psr-4": {
